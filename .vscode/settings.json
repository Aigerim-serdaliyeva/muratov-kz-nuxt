{
  "editor.formatOnSave": true,
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "npm",
  "eslint.validate": [
    "javascript",
    "vue"
  ],
  "[vue]": {
    "editor.formatOnSave": false
  },
  "css.validate": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true
}
